<tr<%= " class=cbac" if result.castlebarac? %> id="<%= dom_id(result) %>">
  <td class="position">
    <%= tag.a(id: dom_id(result)) if result.castlebarac? %>
    <%= result.position %>
  </td>
  <%- if result.bib_numbers_included? %>
    <td class="bib">
      <%= result.bib %>
    </td>
  <%- end %>
  <td class="name">
    <%= name %>
    <%= edit_name %>
  </td>
  <td class="club">
    <%= result.club %>
  </td>
  <%- if result.county_included? %>
    <td class="county">
      <%= result.county %>
    </td>
  <%- end %>
  <%- unless result.timed? %>
    <td class="result">
      <%- if ["DNS", "DNF", "DQ", ""].include? result.result %>
        <%= result.result %>
      <%- else %>
        <%= result.net_time %>
      <%- end %>
    </td>
  <%- end %>
</tr>
