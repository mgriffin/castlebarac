<%= form.label :person %>
<%= form.text_field :person,
  required: true,
  autocomplete: "off",
  data: {
    autocomplete_target: "input"
  } %>
<%= form.hidden_field :person_id, data: { autocomplete_target: "hidden" } %>
<ul class="autocomplete" data-autocomplete-target="results"></ul>
<%= form.label :seconds %>
<%= form.text_field :seconds %>
